{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "rules": {
    "no-unused-vars": "error",
    "no-console": "off",
    "eqeqeq": "error",
    "no-var": "error",
    "prefer-const": "error",
    "no-empty": "error",
    "no-useless-catch": "error"
  },
  "ignorePatterns": ["dist", "node_modules", "workspace"]
}
